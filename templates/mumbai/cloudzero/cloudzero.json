{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Cloudzero stack",
    "Outputs": {
    },
    "Parameters": {
    },
    "Resources": {
      "AppCloudfront":{
        "Type" : "AWS::CloudFront::Distribution",
        "Properties" : {
            "DistributionConfig" : {
                "DefaultCacheBehavior":{
                    "ViewerProtocolPolicy":"allow-all",
                    "TargetOriginId":"app"  
                },
                "Enabled": True,
                "Origins":[
                    {
                        "DomainName" : "Mumba-DsElb-7G4NK4EJVRVM-516364490.ap-south-1.elb.amazonaws.com",
                        "Id" : "Mumbai-DS"
                    }
                ]
            }
          }
      },  
      "CloudfrontLogsSqsQueue": {
        "Type": "AWS::SQS::Queue",
        "Properties": {}
      },
      "CloudfrontLogsSqsPolicy": {
        "Type": "AWS::SQS::QueuePolicy",
        "Properties": {
          "Queues": [
            {
              "Ref": "CloudfrontLogsSqsQueue"
            }
          ],
          "PolicyDocument": {
            "Statement": [
              {
                "Action": [
                  "sqs:SendMessage",
                  "sqs:ReceiveMessage"
                ],
                "Effect": "Allow",
                "Resource": {
                  "Fn::GetAtt": [
                    "CloudfrontLogsSqsQueue",
                    "Arn"
                  ]
                },
                "Principal": {
                  "Service": "s3.amazonaws.com"
                }
              }
            ]
          }
        }
      },
      "CloudfrontLogsS3Bucket": {
        "Type": "AWS::S3::Bucket",
        "Properties": {
            BucketName: "cloudzero-cloudfront-logs"        
        }
      }
    }
  }