{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "DS Base Stack",
    "Outputs": {
      "DsElbTargetGroupArn": {
        "Description": "DS ELB Target Group Arn",
        "Export": {
          "Name": {
            "Fn::Sub": "${AWS::StackName}-Service-DsElbTargetGroupArn"
          }
        },
        "Value": {
          "Ref": "DsElbTargetGroup"
        }
      },
      "DsPrometheusElbTargetGroupArn": {
        "Description": "DS ELB Target Group Arn",
        "Export": {
          "Name": {
            "Fn::Sub": "${AWS::StackName}-Service-DsPrometheusElbTargetGroupArn"
          }
        },
        "Value": {
          "Ref": "DsPrometheusElbTargetGroup"
        }
      }
    },
    "Parameters": {
    },
    "Resources": {
      "CloudfrontLogsSqsQueue": {
        "Type": "AWS::SQS::Queue",
        "Properties": {}
      },
      "CloudfrontLogsSqsPolicy": {
        "Type": "AWS::SQS::QueuePolicy",
        "Properties": {
          "Queues": [
            {
              "Ref": "CloudfrontLogsSqsQueue"
            }
          ],
          "PolicyDocument": {
            "Statement": [
              {
                "Action": [
                  "sqs:SendMessage",
                  "sqs:ReceiveMessage"
                ],
                "Effect": "Allow",
                "Resource": {
                  "Fn::GetAtt": [
                    "CloudfrontLogsSqsQueue",
                    "Arn"
                  ]
                },
                "Principal": {
                  "Service": "s3.amazonaws.com"
                }
              }
            ]
          }
        }
      }
    }
  }